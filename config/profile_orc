# Here are defined all the environment variables required for
# the NRTP Orchestrator

export ORC_BASE=$HOME/trabajo/dec

export ORC_DB_ADAPTER=mysql

export ORC_DATABASE_NAME=test

export ORC_DATABASE_USER=root

export ORC_DATABASE_PASSWORD=1mysql

export ORC_CONFIG=$ORC_BASE/config

export ORC_TMP=/tmp/orc

export PATH=$PATH:$ORC_BASE/code/orc

export RUBYLIB=$ORC_BASE/code

export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/pds_bin/oracle/product/9204/lib

export ORACLE_HOME=/pds_bin/oracle/product/9204

export PATH=$ORACLE_HOME/bin:$PATH

unset USERNAME

