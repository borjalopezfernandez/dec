<?xml version="1.0" encoding="UTF-8"?>

<Config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

   <!-- =========================================================== -->

   <ListInterfaces>

      <Interface>
         <Name>NAOS_MCS_SFTP</Name>
         <LocalOutbox>/tmp/dec/mcsoutbox</LocalOutbox>
         <UploadDir>/tmp</UploadDir>
         <UploadTemp>/tmp</UploadTemp>
      </Interface>
           
   </ListInterfaces>

   <!-- =========================================================== -->

   <ListFiles>

      <File Type="NS1_OPER*">
         <Description></Description>
         <ToList>
            <Interface Name="NAOS_MCS_SFTP"  Compress="NONE" DeliveryMethods="SFTP" />
         </ToList>
      </File>

   </ListFiles>

   <!-- =========================================================== -->

</Config>
