<?xml version="1.0" encoding="UTF-8"?>

<Config>

   <!-- =========================================================== -->

   <ListInterfaces xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">

      <Interface>
         <Name>NAOS_IVV_MCS_SFTP</Name>
         <LocalInbox>/tmp/naos/mcs</LocalInbox>
         <DownloadDirs>
            <Directory DepthSearch="0">/tmp</Directory>
         </DownloadDirs>
         <Switches>
            <DeleteDownloaded>true</DeleteDownloaded>
            <DeleteDuplicated>false</DeleteDuplicated>
            <DeleteUnknown>false</DeleteUnknown>
            <LogDuplicated>false</LogDuplicated>
            <LogUnknown>false</LogUnknown>
         </Switches>
      </Interface>

      <Interface>
         <Name>NASA_CDDIS</Name>
         <LocalInbox>/tmp/dec/ext/nasa_cddis</LocalInbox>
         <DownloadDirs>
		      <Directory DepthSearch="0">/archive/products/iers/finals2000A.data</Directory>
            <Directory DepthSearch="0">/archive/products/iers/tai-utc.dat</Directory>
         </DownloadDirs>
         <Switches>
            <DeleteDownloaded>false</DeleteDownloaded>
            <DeleteDuplicated>false</DeleteDuplicated>
            <DeleteUnknown>false</DeleteUnknown>
            <LogDuplicated>false</LogDuplicated>
            <LogUnknown>false</LogUnknown>
         </Switches>         
      </Interface>
      
      <Interface>
         <Name>CELESTRAK</Name>
         <LocalInbox>/tmp/dec/ext/celestrak</LocalInbox>
         <DownloadDirs>
            <Directory DepthSearch="0">SpaceData/SW-Last5Years.txt</Directory>
         </DownloadDirs>
         <Switches>
            <DeleteDownloaded>false</DeleteDownloaded>
            <DeleteDuplicated>false</DeleteDuplicated>
            <DeleteUnknown>false</DeleteUnknown>
            <LogDuplicated>false</LogDuplicated>
            <LogUnknown>false</LogUnknown>
         </Switches>
      </Interface>  

   </ListInterfaces>

   <!-- =========================================================== -->

   <DownloadRules xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
 
       <File Type="IVV_DEC_TM*.xml">
         <Description>TM decoded from MCS</Description>
         <FromList>
            <Interface>NAOS_IVV_MCS_SFTP</Interface>
         </FromList>
      </File>
     
      <!--
      <File Type="????-??-??T??:??:??*PLA_SBA*">
         <Description>PLA SBA @ MCS</Description>
         <FromList>
            <Interface>NAOS_IVV_MCS_SFTP</Interface>
         </FromList>
      </File>
   -->

      <File Type="finals2000A.data">
         <Description>AUX_NBULA_</Description>
         <FromList>
            <Interface>NASA_CDDIS</Interface>
         </FromList>
      </File>

      <File Type="tai-utc.dat">
         <Description>AUX_NBULC_</Description>
         <FromList>
            <Interface>NASA_CDDIS</Interface>
         </FromList>
      </File>

      <File Type="*Last5Years*">
         <Description>AUX_SFS___</Description>
         <FromList>
            <Interface>CELESTRAK</Interface>
         </FromList>
      </File>   


   </DownloadRules>

   <!-- =========================================================== -->

   <DisseminationRules xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
   
      <ListIntrays>

         <Intray>
            <Name>MCS_DATA</Name>
            <Directory>/tmp/naos/mcs/tm</Directory>
         </Intray>
         
         <Intray>
            <Name>NAOS_IN</Name>
            <Directory>/tmp/dec/int/tmp/</Directory>
         </Intray>         

      </ListIntrays>

      <!-- =============================================== -->

      <ListFilesDisseminated>
    
      <!--
            SORT Rules ranking from more restrictive ones    
            Only the first rule matching the file pattern is applied     
      -->

         <File Type="????-??-??T??:??:??*TO_BE_DEFINED*">
            <HardLink>false</HardLink>
            <ToList>
               <Intray>MCS_DATA</Intray>
            </ToList>
         </File>

         <File Type="finals2000A.data">
            <HardLink>false</HardLink>
            <ToList>
               <Intray>NAOS_IN</Intray>
            </ToList>
         </File>   
   
         <File Type="tai-utc.dat">
            <HardLink>false</HardLink>
            <ToList>
               <Intray>NAOS_IN</Intray>
            </ToList>
         </File>

         <File Type="*Last5Years*">
            <HardLink>false</HardLink>
            <ToList>
               <Intray>NAOS_IN</Intray>
            </ToList>
         </File>           

      </ListFilesDisseminated>
   </DisseminationRules>

   <!-- =========================================================== -->


</Config>
