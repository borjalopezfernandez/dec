      <File Type="*">
      	  <HardLink>False</HardLink>
	         <ToList>
               <Intray>DUPLICATED_FROM_VPMC</Intray>
          </ToList>
      </File>
  
  -->
  
        </ListFilesDisseminated>
   </DisseminationRules>

