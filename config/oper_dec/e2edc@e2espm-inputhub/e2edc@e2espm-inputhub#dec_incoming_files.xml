   <ListInterfaces xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
            
      <Interface>
         <Name>S2PDGS</Name>
         <LocalInbox>/volumes/dec/interfaces/dec_basket.s2pdgs</LocalInbox>
         <DownloadDirs>
		 <Directory DepthSearch="0">/data/DC_BASKET/E2ESPM_OUT/</Directory>
         </DownloadDirs>
      </Interface>

      <Interface>
         <Name>SUPER_TCI</Name>
         <LocalInbox>/volumes/dec/interfaces/dec_basket.tci</LocalInbox>
         <DownloadDirs>
		 <Directory DepthSearch="0">/data/DC_BASKET/E2ESPM_OUT/REP_ARC_TCI</Directory>
         </DownloadDirs>
      </Interface>

   </ListInterfaces>

   <!-- =========================================================== -->



   <!-- =========================================================== -->

<ListFiles xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
 

   <File Type="S2?_*">
      <Description>Brute Force Download for any Sentinel-2 file</Description>
      <FromList>
         <Interface>SUPER_TCI</Interface>
         <Interface>S2PDGS</Interface>
      </FromList>
   </File>

  <File Type="EDR_OPER_SER_SR*">
      <Description>Download of the link execution status for EDRS</Description>
      <FromList>
	 <Interface>S2PDGS</Interface>
      </FromList>
  </File>

  
</ListFiles>

