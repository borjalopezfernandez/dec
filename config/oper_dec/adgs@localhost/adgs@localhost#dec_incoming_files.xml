            </ToList>
         </File>


-->

         <File Type="ims*_*4km*.nc*">
            <HardLink>false</HardLink>
            <ToList>
               <Intray>MINARC_IN</Intray>
            </ToList>
         </File>  


      </ListFilesDisseminated>
   </DisseminationRules>

