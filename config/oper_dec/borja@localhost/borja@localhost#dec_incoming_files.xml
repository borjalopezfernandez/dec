   <DownloadRules xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
 
       <File Type="touch_file*">
         <Description>test file</Description>
         <FromList>
            <Interface>LOCALHOST_FTPS</Interface>
         </FromList>
      </File>

  
      <File Type="test_http*.bin*">
         <Description>test file</Description>
         <FromList>
            <Interface>PARALLEL_HTTP</Interface>
         </FromList>
      </File>
 
      <File Type="tai-utc*">
         <Description>TAI UTC correlation</Description>
         <FromList>
            <Interface>WEBDAV</Interface>
            <Interface>WEBDAV_SECURE</Interface>
            <Interface>PARALLEL_HTTP</Interface>
            <Interface>IERS</Interface>
            <Interface>LOCALHOST_NOT_SECURE</Interface>
            <Interface>FILESYSTEM</Interface>
         </FromList>
      </File>

      <File Type="Leap_Second.dat">
         <Description>IERS TAI UTC Time Correlation</Description>
         <FromList>
            <Interface>LEAP</Interface>
         </FromList>
      </File>

      <File Type="csv">
         <Description>Comma Separated Value</Description>
         <FromList>
            <Interface>ECDC</Interface>
         </FromList>
      </File>


      <File Type="finals2000A.daily">
         <Description>EOP parameters</Description>
         <FromList>
            <Interface>IERS</Interface>
         </FromList>
      </File>

      <File Type="finals2000A.data">
         <Description>EOP parameters</Description>
         <FromList>
            <Interface>IERS</Interface>
         </FromList>
      </File>


      <File Type="gpsrapid*">
         <Description>GPS data</Description>
         <FromList>
            <Interface>IERS</Interface>
         </FromList>
      </File>

      <File Type="finals.a*">
         <Description>TAI UTC correlation</Description>
         <FromList>
            <Interface>WEBDAV</Interface>
            <Interface>WEBDAV_SECURE</Interface>
            <Interface>IERS</Interface>
            <Interface>LOCALHOST_NOT_SECURE</Interface>
            <Interface>PARALLEL_HTTP</Interface>
            <Interface>FILESYSTEM</Interface>
         </FromList>
      </File>


<!--

igswwwwd.sp3

https://cddis.nasa.gov/Data_and_Derived_Products/GNSS/gnss_igsclk5.html

WWWW/igsWWWWD.clk.Z

> WWWW   GPS week
> D      day of week (0-6, 7 indicates weekly)


-->



      <File Type="igs?????.*">
         <Description>International GPS Service / GPS Orbits igswwwwd.sp3.Z</Description>
         <FromList>
            <Interface>IGS</Interface>
         </FromList>
      </File>



      <File Type="ig??????.clk_30s*">
         <Description>International GPS Service / GPS Clocks igswwwwd.sp3.Z</Description>
         <FromList>
            <Interface>IGS</Interface>
         </FromList>
      </File>

      <File Type="ig??????.clk_30s*">
         <Description>International GPS Service / GPS Clocks igswwwwd.sp3.Z</Description>
         <FromList>
            <Interface>IGS</Interface>
         </FromList>
      </File>

      <File Type="brdc????.??n.Z">
         <Description>International GPS Service / GPS navigation message brdcdddf.yyn.Z</Description>
         <FromList>
            <Interface>IGS</Interface>
         </FromList>
      </File>

      <File Type="sentinel3*">
         <Description>Satellite Laser Ranging data</Description>
         <FromList>
            <Interface>ILRS</Interface>
         </FromList>
      </File>

      <File Type="????????RSGA.txt">
         <Description>Report Solar Geophysical Activity flux and Geomagnetic activity</Description>
         <FromList>
            <Interface>NOAA</Interface>
         </FromList>
      </File>
     
      <File Type="S2?_*">
         <Description>Sentinel-2 Ground Segment</Description>
         <FromList>
            <Interface>LOCALHOST_SECURE</Interface>
            <Interface>LOCALHOST_NOT_SECURE</Interface>
            <Interface>SCIHUB</Interface>
         </FromList>
      </File>

      <File Type="S3__GN_1_LSC_AX*">
         <Description>Sentinel-3 Leap Second</Description>
         <FromList>
            <Interface>PUBLISH_S3</Interface>
         </FromList>
      </File>

      <File Type="S3__GN_1_NAV_AX*">
         <Description>Sentinel-3 IGS Broadcast Daily</Description>
         <FromList>
            <Interface>PUBLISH_S3</Interface>
         </FromList>
      </File>

      <File Type="S3__GN_1_EOP_AX*">
         <Description>IERS Earth Orientation Parameters</Description>
         <FromList>
            <Interface>PUBLISH_S3</Interface>
         </FromList>
      </File>

      <File Type="S3__GN_1_SAC_AX*">
         <Description>NOAA Solar Activity Report</Description>
         <FromList>
            <Interface>PUBLISH_S3</Interface>
         </FromList>
      </File>

     
   </DownloadRules>

