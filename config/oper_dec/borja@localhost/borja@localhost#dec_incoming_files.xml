   <DisseminationRules xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
   
      <ListIntrays>

         <Intray>
            <Name>DIAS</Name>
            <Directory>/tmp/dec_final_dissemination/DIAS/</Directory>
         </Intray>


         <Intray>
            <Name>FINAL_S3</Name>
            <Directory>/tmp/dec_final_dissemination/S3/</Directory>
         </Intray>

         <Intray>
            <Name>ARCHIVE</Name>
            <Directory>/tmp/dec_final_dissemination/archive/</Directory>
            <!--
            <Compress>7z</Compress>
            -->
         </Intray>

         <Intray>
            <Name>S3_IN</Name>
            <Directory>/tmp/dec_local_dissemination/S3/</Directory>
            <!--
            <Execute>auxConverter -f %F -d /tmp/aux_tmp</Execute>
            -->
         </Intray>

         <Intray>
            <Name>S2ALL</Name>
            <Directory>/tmp/dec_local_dissemination/S2ALL</Directory>
         </Intray>

         <Intray>
            <Name>S2A</Name>
            <Directory>/tmp/dec_local_dissemination/S2A</Directory>
            <Compress>7z</Compress>
            <!--
            <Execute>7z</Execute>
            -->
         </Intray>

         <Intray>
            <Name>S2B</Name>
            <Directory>/tmp/dec_local_dissemination/S2B</Directory>
            <!--
            <Execute>7z</Execute>
            -->
         </Intray>

         <Intray>
            <Name>GPS</Name>
            <Directory>/tmp/dec_local_dissemination/GPS</Directory>
         </Intray>
      
         <Intray>
            <Name>DEC_REPORTS</Name>
            <Directory>/tmp/dec_local_dissemination/DEC_Reports</Directory>
         </Intray>

         <Intray>
            <Name>DEC_REPORTS_GRAVEYARD</Name>
            <Directory>/tmp/dec_local_dissemination/DEC_Reports_Graveyard</Directory>
         </Intray>
 
         <Intray>
            <Name>POD_IN</Name>
            <Directory>/tmp/dec_tmp/dec/out_basket_pod_push/http</Directory>
            <!--
            <Execute>decSend2Interface -m POD_PUSH</Execute>
            -->
         </Intray>

      
                
      </ListIntrays>

