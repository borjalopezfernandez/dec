<?xml version="1.0" encoding="UTF-8"?>
<Configuration>
   <Project>
      <Name>MCF Facility</Name>
      <Id>MCF</Id>
      <Mission>MCF</Mission>
   </Project>
   <Filters>
      <OutgoingFilters>
         <Filter>*</Filter>
      </OutgoingFilters>
   </Filters>
   <Options>
      <DeleteSourceFiles>true</DeleteSourceFiles>
      <UploadFilePrefix>__</UploadFilePrefix>
      <UploadDirs>true</UploadDirs>
   </Options>
   <GlobalOutbox>$DMC_CONF_PATH/../tmp</GlobalOutbox>
   <SatPrefix>MAC</SatPrefix>
      <ReportDir>$DC_CONF_PATH/../tmp</ReportDir>
   <Reports>
      
      <Report Name="DeliveredFiles">
         <Enabled>false</Enabled>
         <Desc>List of Files Delivered</Desc>
         <FileType>REP_DMCOUT</FileType>
      </Report>

      <Report Name="EmergencyDeliveredFiles">
         <Enabled>false</Enabled>
         <Desc>List of Files Delivered in Emergency Mode</Desc>
         <FileType>REP_DMC_EM</FileType>
      </Report>

   </Reports>
</Configuration>

