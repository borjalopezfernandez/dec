   <List_of_Breakpoints>
   <!--
      <breakpoint key="Scenes_Vector_input" value="false" mandatory="false" valueType="boolean"/>
      <breakpoint key="Scenes_Normalized" value="false" mandatory="false" valueType="boolean"/>
