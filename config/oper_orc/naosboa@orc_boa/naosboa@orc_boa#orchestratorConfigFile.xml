	      <Directory>[update]/data/l0-in/</Directory>
      </DataProvider>

      <DataProvider isTriggerType="yes">
	      <DataType>PLA_MAINT</DataType>
	      <FileType>*PLA_MAINT*</FileType>
	      <Directory>[update]/data/l0-in/</Directory>
      </DataProvider>      

      <DataProvider isTriggerType="yes">
	      <DataType>ORB_OEM</DataType>
	      <FileType>*ORB_OEM*</FileType>
	      <Directory>[update]/data/l0-in/</Directory>
      </DataProvider>   

      <DataProvider isTriggerType="yes">
	      <DataType>SCH_REQ</DataType>
	      <FileType>*ksat_schedule_request*</FileType>
	      <Directory>[update]/data/l0-in/</Directory>
      </DataProvider>
      
      <DataProvider isTriggerType="no">
	      <DataType>ADA_DOP___</DataType>
	      <FileType>*TRK_*RAN*.GEO</FileType>
	      <Directory>[update]/data/l0-in/</Directory>
      </DataProvider>

      <DataProvider isTriggerType="no">
	      <DataType>ADA_TDA___</DataType>
	      <FileType>*TRK_*ANG*.GEO</FileType>
	      <Directory>[update]/data/l0-in/</Directory>
      </DataProvider>

      <DataProvider isTriggerType="no">
	      <DataType>PLA_GSP___</DataType>
	      <FileType>*PLA_GSP___*</FileType>
	      <Directory>[update]/data/l0-in/</Directory>
      </DataProvider>

      <DataProvider isTriggerType="no">
	      <DataType>ORB_OPM___</DataType>
	      <FileType>*ORB_OPM___*</FileType>
	      <Directory>[update]/data/l0-in/</Directory>
      </DataProvider>

      <DataProvider isTriggerType="no">
	      <DataType>CNF_OAT___</DataType>
	      <FileType>*CNF_OAT___*</FileType>
	      <Directory>[update]/data/l0-in/</Directory>
      </DataProvider>

      <DataProvider isTriggerType="no">
	      <DataType>CNF_CONT__</DataType>
	      <FileType>*CNF_CONT__*</FileType>
	      <Directory>[update]/data/l0-in/</Directory>
      </DataProvider>


   </List_of_DataProviders>


   <!-- ====================================================== -->

<!-- 

1. AUX_TLE
2. ORB_SOERT
4. PLA_SCR
5. PLA_SBA
6. PLA_OSC
7. PLA_MAN
8. PLA_MAINT
9. ORB_OEM
10. ksat_schedule_request



-->

   <List_of_PriorityRules>
      <PriorityRule rank="1"     class="DataType" type="AUX_TLE"        sort="DESC" />
      <PriorityRule rank="2"     class="DataType" type="ORB_SOERT"      sort="DESC" />
      <PriorityRule rank="3"     class="DataType" type="PLA_SCR"        sort="DESC" />
      <PriorityRule rank="4"     class="DataType" type="PLA_SBA"        sort="DESC" />
      <PriorityRule rank="5"     class="DataType" type="PLA_OSC"        sort="DESC" />
      <PriorityRule rank="6"     class="DataType" type="PLA_MAN"        sort="DESC" />
      <PriorityRule rank="7"     class="DataType" type="PLA_MAINT"      sort="DESC" />
      <PriorityRule rank="8"     class="DataType" type="ORB_OEM"        sort="DESC" />
      <PriorityRule rank="9"     class="DataType" type="SCH_REQ"        sort="DESC" />

   </List_of_PriorityRules>

   <!-- ====================================================== -->
  
   <List_of_ProcessingRules>
   
      <ProcessingRule dataType="AUX_TLE" triggerType="AUX_TLE" coverage="SAME">
