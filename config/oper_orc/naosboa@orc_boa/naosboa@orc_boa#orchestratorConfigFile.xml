<OrchestratorConfiguration>


   <List_of_DataProviders>


<!-- 

1. AUX_TLE
2. ORB_SOERT
4. PLA_SCR
5. PLA_SBA
6. PLA_OSC
7. PLA_MAN
8. PLA_MAINT
9. ORB_OEM
10. ksat_schedule_request

-->

      <DataProvider isTriggerType="yes">
	      <DataType>AUX_TLE</DataType>
	      <FileType>*AUX_TLE*</FileType>
	      <Directory>[update]/data/l0-in/</Directory>
      </DataProvider>

      <DataProvider isTriggerType="yes">
	      <DataType>ORB_SOERT</DataType>
	      <FileType>*ORB_SOERT*</FileType>
	      <Directory>[update]/data/l0-in/</Directory>
      </DataProvider>

      <DataProvider isTriggerType="yes">
	      <DataType>PLA_SCR</DataType>
	      <FileType>*PLA_SCR*</FileType>
	      <Directory>[update]/data/l0-in/</Directory>
      </DataProvider>

      <DataProvider isTriggerType="yes">
	      <DataType>PLA_SBA</DataType>
	      <FileType>*PLA_SBA*</FileType>
	      <Directory>[update]/data/l0-in/</Directory>
      </DataProvider>

      <DataProvider isTriggerType="yes">
	      <DataType>PLA_OSC</DataType>
	      <FileType>*PLA_OSC*</FileType>
	      <Directory>[update]/data/l0-in/</Directory>
      </DataProvider>

      <DataProvider isTriggerType="yes">
	      <DataType>PLA_MAN</DataType>
